{
  "name": "record-js",
  "version": "0.0.2",
  "description": "JavaScript implementation of a universal blockchain OS",
  "main": "dist/r.js",
  "scripts": {
    "prepublish": "npm run dist",
    "test": "babel-node test/index.js && npm run lint",
    "lint": "find src/ -name '*.js' -print0 | xargs -0 -n1 eslint",
    "console": "babel-node bin/console.js",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "dist": "webpack --optimize-minimize src/r.js dist/r.js"
  },
  "bin": {
    "record": "bin/record.js"
  },
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checle/record-js.git"
  },
  "author": "Filip Dal√ºge <daluege@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checle/record-js/issues"
  },
  "homepage": "https://github.com/checle/record-js#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "devtool": "^2.0.3",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "tape": "^4.6.0",
    "webpack": "^1.13.1"
  }
}
