{
  "name": "polycode",
  "version": "0.1.0",
  "description": "Blockchain OS",
  "main": "dist/index",
  "bin": {
    "record": "dist/bin/record.js"
  },
  "scripts": {
    "prepublish": "npm run clean && typings install && npm run dist",
    "test": "ts-node node_modules/tape/bin/tape src/**/test/*.ts src/**/test.ts",
    "lint": "tslint src/**/*.ts",
    "build": "tsc",
    "dist": "npm run build && webpack --optimize-minimize dist/index dist/browser",
    "clean": "rm -rf dist",
    "record": "ts-node src/bin/record.ts"
  },
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checle/record-js.git"
  },
  "author": "Filip Dal√ºge <daluege@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checle/record-js/issues"
  },
  "homepage": "https://github.com/checle/record-js",
  "devDependencies": {
    "tape": "^4.6.0",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^2.1.1",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "cash": "^0.8.0",
    "commander": "^2.9.0",
    "jsvm": "^0.9.0",
    "operate": "0.0.3"
  }
}
