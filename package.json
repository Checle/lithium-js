{
  "name": "record-js",
  "version": "0.0.1",
  "description": "Record tree for JavaScript",
  "main": "src/r.js",
  "scripts": {
    "prepublish": "",
    "test": "babel-node $(git ls-files test/**/*.js) && eslint $(git ls-files src/*.js src/**/*.js)",
    "console": "babel-node bin/console.js",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "dist": "webpack --optimize-minimize src/r.js dist/r.js"
  },
  "bin": {
    "record": "bin/record.js"
  },
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checle/r.js.git"
  },
  "author": "Filip Dal√ºge <daluege@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checle/r.js/issues"
  },
  "homepage": "https://github.com/checle/r.js#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "devtool": "^2.0.3",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "tape": "^4.6.0",
    "webpack": "^1.13.1"
  }
}
