{
  "name": "checle-ts-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "postpublish": "npm run deploy",
    "test": "tsc && ava dist/**/test/*.js dist/**/test.js",
    "lint": "tslint src/**/*.ts",
    "build": "rollup -c",
    "dist": "npm run build",
    "clean": "rm -rf dist",
    "deploy": "npm run dist && gh-pages -d dist -m \"Deploy $(npm view checle-ts-boilerplate version)\" || true",
    "debug": "devtool test/index.js"
  },
  "bin": {
    "checle-ts-boilerplate": "bin/index.js"
  },
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checle/checle-ts-boilerplate.git"
  },
  "author": "Filip Dal√ºge <daluege@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checle/checle-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/checle/checle-ts-boilerplate",
  "devDependencies": {
    "@types/node": "^7.0.13",
    "ava": "^0.19.1",
    "devtool": "^2.0.3",
    "gh-pages": "^0.11.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^3.2.2",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^2.2.2"
  }
}
